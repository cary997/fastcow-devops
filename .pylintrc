[MAIN]
ignore=CVS
ignore-paths=^.*\\alembic.*|^.*/alembic.*,^.*\\initial_models.py|^.*/initial_models.py,^.*devtest.*
ignore-patterns=
ignored-modules=
load-plugins=
py-version=3.12

[FORMAT]
max-line-length=120
max-module-lines=1000

[MESSAGES CONTROL]
disable=raw-checker-failed,
        bad-inline-option,
        locally-disabled,
        file-ignored,
        suppressed-message,
        useless-suppression,
        deprecated-pragma,
        use-symbolic-message-instead,
        use-implicit-booleaness-not-comparison-to-string,
        use-implicit-booleaness-not-comparison-to-zero,
        invalid-name,
        missing-module-docstring,
        wrong-import-order,
        trailing-whitespace,
        raise-missing-from,
        missing-function-docstring